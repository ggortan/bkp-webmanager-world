{
  "_README": "Copie este arquivo para config.json e ajuste os valores",

  "api_url": "https://backup.seudominio.com",
  "api_token": "COLE_AQUI_A_API_KEY_DO_CLIENTE",
  "host_name": "SRV-EXEMPLO-01",

  "telemetry": {
    "enabled": true,
    "interval_minutes": 5
  },

  "backup": {
    "check_interval_minutes": 15,
    "collectors": ["wsb", "veeam"]
  },

  "routines": [
    {
      "_comment": "Rotina para Windows Server Backup - obtém do cmdlet Get-WBJob",
      "routine_key": "rtk_COLE_A_ROUTINE_KEY_AQUI",
      "source": "wsb",
      "job_name": null
    },
    {
      "_comment": "Rotina para job específico do Veeam - obtém do cmdlet Get-VBRJob",
      "routine_key": "rtk_COLE_OUTRA_ROUTINE_KEY",
      "source": "veeam",
      "job_name": "Backup Diário - Servidor Arquivos"
    }
  ]
}
